import java.sql.*;

public class UsersDao {
	
	public static Connection getConnection(){
        Connection con=null;
        try{
            Class.forName("com.mysql.jdbc.Driver");
            con= DriverManager.getConnection("jdbc:mysql://localhost/ELibrary","root","");
        }catch(Exception e){System.out.println(e);}
        return con;
    }

}
